<%- layout('layout') %>
<% block('view', '<input id="_moduleViewName" type="hidden" value="classes_schedules_stu">') %>

<div style="width:1100px; margin: 10px auto;">
    <input type="hidden" id="tabname" value="<%= tabname %>">
    <input type="hidden" id="userid" value="<%= page.user.id %>">
    <input type="hidden" id="userType" value="<%= page.user.type || 1 %>">
    <input type="hidden" id="schoolid" value="<%= page.user.schoolid %>">
    <input type="hidden" id="code" value="<%= page.user.code %>">
    <ul class="nav nav-tabs" id="schedules-stu-tab">
        <li><a href="#myclass" data-toggle="tab" data-bypass>
                我的班级
            </a></li>
        <li><a href="#myschedule" data-toggle="tab" data-bypass>
                我的课表
            </a></li>
    </ul>

    <div class="tab-content">
        <div class="tab-pane" id="myclass">
            <% include s-class-list.ejs %>
        </div>
        <div class="tab-pane" id="myschedule">
            <% include s-schedules.ejs %>
        </div>
    </div>
</div>




