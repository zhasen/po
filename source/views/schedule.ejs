<div class="lp_btnp_Bg">
    <div class="lpath_page">
        <dl class="btn_part fix">
            <dd class="f_r">
                <a href="/scheduledl-<%= schoolid %>-<%= classcode %>" data-bypass
                   class="btn_download_schedule lp_pbg"></a></dd>
        </dl>
        <% include s-schedules.ejs %>
    </div>
</div>
