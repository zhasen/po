<%- layout('layout') %>
<% block('view', '<input id="_moduleViewName" type="hidden" value="schedule">') %>

<input type="hidden" id="userid" value="<%= page.user.id %>">
<input type="hidden" id="userType" value="<%= page.user.type || 1 %>">
<input type="hidden" id="schoolid" value="<%= page.user.schoolid %>">
<input type="hidden" id="code" value="<%= page.user.code %>">
<!--banner start-->
<div class="learn_path_focus lp_classmain_focus">
    <div class="focus_bg">
        <div class="lp_thisrec_c">
            <dl class="lp_reccourse">
                <dt class="f_l">
                <p><a href="javascript:void(0);"><%= page.classData.ClassName %></a></p>
                <strong class="fs_14"><%= page.classData.ClassCode %></strong>
                <%= page.classData.poBeginDate %>-<%= page.classData.poEndDate %>
                </dt>
                <dd class="f_r">
                    <div class="rec_level fs_14">预备</div>
                    <div class="rec_status fc_3dc">
                        <font class="lp_sicons" id="icons_ing"></font>
                        <%= page.classData.ClassStatusText %>
                    </div>
                </dd>
            </dl>
        </div>
        <div class="lp_view_allcs">
            <font class="lp_sicons icon_up"></font>
        </div>
    </div>
</div>
<!--banner end-->

<!--课表导航start-->
